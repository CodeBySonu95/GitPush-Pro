<androidx.drawerlayout.widget.DrawerLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/drawer_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#0B0E0B">
	<RelativeLayout
		android:id="@+id/relativelayout2"
		android:layout_width="match_parent"
		android:layout_height="match_parent">
		<LinearLayout
			android:id="@+id/linear_toolbar"
			android:layout_width="match_parent"
			android:layout_height="65dp"
			android:gravity="center_vertical"
			android:orientation="horizontal"
			android:paddingHorizontal="16dp">
			<ImageView
				android:id="@+id/img_menu"
				android:layout_width="24dp"
				android:layout_height="24dp"
				android:src="@drawable/icon_menu"
				android:scaleType="fitXY"
				app:tint="#FFFFFF" />
			<TextView
				android:id="@+id/textview14"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:text="Activity"
				android:textSize="20sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF"
				android:layout_weight="1"
				android:layout_marginStart="16dp" />
			<LinearLayout
				android:id="@+id/linear10"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				android:gravity="end"
				android:layout_marginEnd="12dp">
				<TextView
					android:id="@+id/repo_connected_tv"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="CONNECTED"
					android:textSize="10sp"
					android:textStyle="bold"
					android:textColor="#52D02E" />
				<TextView
					android:id="@+id/website_branch_tv"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="github.com/main"
					android:textSize="11sp"
					android:textColor="#666666" />
			</LinearLayout>
			<com.google.android.material.card.MaterialCardView
				android:id="@+id/cardview3"
				android:layout_width="38dp"
				android:layout_height="38dp"
				app:cardBackgroundColor="#1B2A16"
				app:cardCornerRadius="10dp"
				app:cardElevation="0dp"
				app:strokeWidth="0dp">
				<ImageView
					android:id="@+id/imageview3"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:padding="10dp"
					android:src="@drawable/icon_terminal"
					android:scaleType="fitCenter"
					app:tint="#52D02E" />
			</com.google.android.material.card.MaterialCardView>
		</LinearLayout>
		<androidx.core.widget.NestedScrollView
			android:id="@+id/vscroll2"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:scrollbars="none"
			android:layout_above="@id/linear_footer"
			android:layout_below="@id/linear_toolbar">
			<LinearLayout
				android:id="@+id/linear11"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				android:paddingHorizontal="16dp">
				<LinearLayout
					android:id="@+id/linear12"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="16dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<View
						android:id="@+id/stutus_dot"
						android:layout_width="6dp"
						android:layout_height="6dp"
						android:background="#52D02E" />
					<TextView
						android:id="@+id/tv_push_status"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="ACTIVE PUSHES"
						android:textSize="12sp"
						android:textStyle="bold"
						android:textColor="#52D02E"
						android:layout_marginStart="8dp"
						android:letterSpacing="0.1" />
				</LinearLayout>
				<com.google.android.material.card.MaterialCardView
					android:id="@+id/cardview4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="12dp"
					app:cardBackgroundColor="#151815"
					app:cardCornerRadius="20dp"
					app:strokeWidth="1dp"
					app:strokeColor="#222222"
					app:cardElevation="0dp">
					<LinearLayout
						android:id="@+id/linear14"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="vertical">
						<RelativeLayout
							android:id="@+id/relativelayout4"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="16dp">
							<com.google.android.material.card.MaterialCardView
								android:id="@+id/icon_upload_bg"
								android:layout_width="36dp"
								android:layout_height="36dp"
								app:cardBackgroundColor="#1B2A16"
								app:cardCornerRadius="8dp"
								app:cardElevation="0dp">
								<ImageView
									android:id="@+id/file_type_icon"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:padding="8dp"
									android:src="@drawable/icon_upload_file"
									android:scaleType="fitCenter"
									app:tint="#52D02E" />
							</com.google.android.material.card.MaterialCardView>
							<LinearLayout
								android:id="@+id/linear15"
								android:layout_width="250dp"
								android:layout_height="wrap_content"
								android:orientation="vertical"
								android:layout_marginStart="12dp"
								android:layout_toEndOf="@id/icon_upload_bg">
								<TextView
									android:id="@+id/txt_file_name"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="Uploading update.apk"
									android:textSize="15sp"
									android:textStyle="bold"
									android:textColor="#FFFFFF"
									android:singleLine="true"
									android:maxLines="1"
									android:ellipsize="end" />
								<TextView
									android:id="@+id/branch_info_tv"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:text="Branch: production-v2"
									android:textSize="12sp"
									android:textColor="#666666" />
							</LinearLayout>
							<TextView
								android:id="@+id/percentage_tv"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="78%"
								android:textSize="14sp"
								android:textStyle="bold"
								android:textColor="#52D02E"
								android:layout_alignParentEnd="true" />
							<ProgressBar
								android:id="@+id/progressbar1"
								android:layout_width="match_parent"
								android:layout_height="6dp"
								android:layout_marginTop="16dp"
								android:progress="78"
								android:indeterminate="false"
								style="?android:progressBarStyleHorizontal"
								android:progressDrawable="@drawable/custom_progress_green"
								android:layout_below="@id/icon_upload_bg" />
						</RelativeLayout>
						<LinearLayout
							android:id="@+id/linear16"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="12dp"
							android:background="#000000"
							android:orientation="vertical">
							<TextView
								android:id="@+id/terminal_output_tv"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:text="[$] Welcome back.\n[$] System ready. Waiting for files..."
								android:textSize="11sp"
								android:textColor="#9E9E9E"
								android:fontFamily="monospace"
								android:lineSpacingExtra="4dp"
								android:textIsSelectable="true" />
							<View
								android:id="@+id/view17"
								android:layout_width="2dp"
								android:layout_height="15dp"
								android:layout_marginTop="4dp"
								android:background="#52D02E" />
						</LinearLayout>
						<RelativeLayout
							android:id="@+id/relativelayout5"
							android:layout_width="match_parent"
							android:layout_height="48dp"
							android:paddingHorizontal="16dp">
							<TextView
								android:id="@+id/textview22"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="LIVE TERMINAL OUTPUT"
								android:textSize="10sp"
								android:textStyle="bold"
								android:textColor="#444444"
								android:layout_centerVertical="true" />
							<TextView
								android:id="@+id/btn_clear"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Clear"
								android:textSize="12sp"
								android:textStyle="bold"
								android:textColor="#FFFFFF"
								android:layout_alignParentEnd="true"
								android:layout_centerVertical="true" />
						</RelativeLayout>
					</LinearLayout>
				</com.google.android.material.card.MaterialCardView>
				<RelativeLayout
					android:id="@+id/relativelayout6"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="32dp">
					<TextView
						android:id="@+id/textview23"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="COMMIT HISTORY"
						android:textSize="12sp"
						android:textStyle="bold"
						android:textColor="#666666"
						android:letterSpacing="0.1" />
					<com.google.android.material.card.MaterialCardView
						android:id="@+id/btn_short"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						app:cardBackgroundColor="#1B2A16"
						app:cardCornerRadius="12dp"
						app:strokeWidth="0dp"
						app:cardElevation="0dp"
						android:layout_alignParentEnd="true">
						<LinearLayout
							android:id="@+id/linear2"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:gravity="center_vertical"
							android:orientation="horizontal"
							android:paddingHorizontal="10dp"
							android:paddingVertical="4dp">
							<TextView
								android:id="@+id/filter_tv"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Newest First"
								android:textSize="10sp"
								android:textStyle="bold"
								android:textColor="#52D02E" />
							<ImageView
								android:id="@+id/img_filter"
								android:layout_width="12dp"
								android:layout_height="12dp"
								android:src="@drawable/icon_filter_list"
								android:scaleType="fitCenter"
								android:layout_marginStart="6dp"
								app:tint="#52D02E" />
						</LinearLayout>
					</com.google.android.material.card.MaterialCardView>
				</RelativeLayout>
				<androidx.recyclerview.widget.RecyclerView
					android:id="@+id/recyclerview1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="12dp"
					android:nestedScrollingEnabled="false"
					tools:listitem="@layout/item_commit" />
			</LinearLayout>
		</androidx.core.widget.NestedScrollView>
		<LinearLayout
			android:id="@+id/linear_footer"
			android:layout_width="match_parent"
			android:layout_height="50dp"
			android:background="#0B0E0B"
			android:gravity="center_vertical"
			android:orientation="horizontal"
			android:paddingHorizontal="20dp"
			android:layout_alignParentBottom="true">
			<View
				android:id="@+id/view18"
				android:layout_width="8dp"
				android:layout_height="8dp"
				android:background="@drawable/circle_green" />
			<TextView
				android:id="@+id/textview25"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:text="SYSTEM STATUS: OPTIMAL"
				android:textSize="11sp"
				android:textStyle="bold"
				android:textColor="#9E9E9E"
				android:layout_weight="1"
				android:layout_marginStart="10dp" />
			<TextView
				android:id="@+id/developer"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="CodeBySonu"
				android:textSize="11sp"
				android:textColor="#444444" />
		</LinearLayout>
		<com.google.android.material.card.MaterialCardView
			android:id="@+id/fab_upload"
			android:layout_width="56dp"
			android:layout_height="56dp"
			android:layout_marginBottom="20dp"
			app:cardBackgroundColor="#52D02E"
			android:layout_marginEnd="20dp"
			app:cardCornerRadius="18dp"
			app:cardElevation="8dp"
			app:strokeWidth="0dp"
			android:focusable="true"
			android:layout_above="@id/linear_footer"
			android:layout_alignParentEnd="true">
			<ImageView
				android:id="@+id/imageview_fab_icon"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:padding="15dp"
				android:src="@drawable/ic_upload_bar"
				android:scaleType="fitCenter"
				app:tint="#000000" />
		</com.google.android.material.card.MaterialCardView>
	</RelativeLayout>
	<include
		layout="@layout/custom_drawer"
		android:layout_width="280dp"
		android:layout_height="match_parent"
		android:layout_gravity="start" />
</androidx.drawerlayout.widget.DrawerLayout>
