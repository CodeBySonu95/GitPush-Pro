<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical"
	android:paddingBottom="12dp">
	<com.google.android.material.card.MaterialCardView
		android:id="@+id/cardview6"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:cardBackgroundColor="#151815"
		app:cardCornerRadius="18dp"
		app:strokeWidth="1dp"
		app:strokeColor="#222222"
		app:cardElevation="0dp">
		<RelativeLayout
			android:id="@+id/relativelayout9"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="16dp">
			<com.google.android.material.card.MaterialCardView
				android:id="@+id/card_status_icon"
				android:layout_width="32dp"
				android:layout_height="32dp"
				app:cardBackgroundColor="#1B2A16"
				app:cardCornerRadius="16dp"
				app:cardElevation="0dp"
				app:strokeWidth="0dp">
				<ImageView
					android:id="@+id/img_status"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:src="@drawable/icon_check_circle"
					android:scaleType="fitXY"
					app:tint="#52D02E"
					android:focusable="false" />
			</com.google.android.material.card.MaterialCardView>
			<LinearLayout
				android:id="@+id/linear22"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				android:layout_marginStart="14dp"
				android:layout_toEndOf="@id/card_status_icon">
				<RelativeLayout
					android:id="@+id/relativelayout10"
					android:layout_width="match_parent"
					android:layout_height="wrap_content">
					<TextView
						android:id="@+id/stutus_tv"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Commit Push successful"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#FFFFFF"
						android:focusable="false" />
					<TextView
						android:id="@+id/txt_time"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="14:20"
						android:textSize="11sp"
						android:textColor="#444444"
						android:focusable="false"
						android:layout_alignParentEnd="true" />
				</RelativeLayout>
				<LinearLayout
					android:id="@+id/linear23"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="4dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/file_icon"
						android:layout_width="12dp"
						android:layout_height="12dp"
						android:src="@drawable/icon_merge"
						android:scaleType="fitCenter"
						app:tint="#666666"
						android:focusable="false" />
					<TextView
						android:id="@+id/file_count_tv"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="4 files"
						android:textSize="12sp"
						android:textColor="#9E9E9E"
						android:layout_marginStart="6dp"
						android:focusable="false" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear24"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dp"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/txt_tag_branch"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="@drawable/bg_tag_dark"
						android:text="main"
						android:textSize="10sp"
						android:textColor="#666666"
						android:paddingHorizontal="8dp"
						android:paddingVertical="2dp"
						android:focusable="false" />
					<TextView
						android:id="@+id/txt_tag_hash"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="f8a2e1d"
						android:textSize="10sp"
						android:textColor="#444444"
						android:layout_marginStart="8dp"
						android:fontFamily="monospace"
						android:focusable="false" />
				</LinearLayout>
				<TextView
					android:id="@+id/txt_error_msg"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="10dp"
					android:padding="8dp"
					android:background="@drawable/bg_error_light"
					android:text="Error: Connection timeout during upload."
					android:textSize="11sp"
					android:textColor="#FF5252"
					android:visibility="gone"
					android:focusable="false"
					android:textIsSelectable="true" />
			</LinearLayout>
		</RelativeLayout>
	</com.google.android.material.card.MaterialCardView>
</LinearLayout>
