<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/linear_root"
	android:layout_width="280dp"
	android:layout_height="match_parent"
	android:layout_gravity="start"
	android:orientation="vertical"
	android:background="#0B0E0B"
	android:clickable="true"
	android:focusable="true">
	<LinearLayout
		android:id="@+id/linear20"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="24dp"
		android:padding="20dp"
		android:gravity="center_vertical"
		android:orientation="horizontal">
		<com.google.android.material.card.MaterialCardView
			android:id="@+id/cardview4"
			android:layout_width="60dp"
			android:layout_height="60dp"
			app:cardBackgroundColor="#FFD1B3"
			app:cardCornerRadius="16dp"
			app:cardElevation="0dp"
			app:strokeWidth="0dp">
			<ImageView
				android:id="@+id/imageview14"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:src="@drawable/icon_person"
				android:scaleType="centerInside"
				android:focusable="false" />
		</com.google.android.material.card.MaterialCardView>
		<LinearLayout
			android:id="@+id/linear21"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:layout_marginStart="16dp">
			<TextView
				android:id="@+id/username_tv"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="GitPush_Pro"
				android:textSize="18sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF"
				android:focusable="false" />
			<TextView
				android:id="@+id/repo_path_tv"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="2dp"
				android:text="username/repo"
				android:textSize="11sp"
				android:textColor="#666666"
				android:focusable="false" />
		</LinearLayout>
	</LinearLayout>
	<View
		android:id="@+id/view22"
		android:layout_width="match_parent"
		android:layout_height="1dp"
		android:background="#222222"
		android:layout_marginHorizontal="20dp"
		android:layout_marginVertical="10dp" />
	<ScrollView
		android:id="@+id/vscroll4"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:scrollbars="none">
		<LinearLayout
			android:id="@+id/linear23"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:paddingHorizontal="12dp">
			<com.google.android.material.card.MaterialCardView
				android:id="@+id/nav_terminal"
				android:layout_width="match_parent"
				android:layout_height="52dp"
				android:layout_marginBottom="4dp"
				app:cardBackgroundColor="@android:color/transparent"
				app:cardCornerRadius="26dp"
				app:cardElevation="0dp"
				app:strokeWidth="0dp">
				<LinearLayout
					android:id="@+id/linear24"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:gravity="center_vertical"
					android:paddingHorizontal="16dp">
					<ImageView
						android:id="@+id/img_terminal"
						android:layout_width="20dp"
						android:layout_height="20dp"
						android:src="@drawable/icon_terminal"
						android:scaleType="fitXY"
						android:focusable="false"
						app:tint="#9E9E9E" />
					<TextView
						android:id="@+id/txt_terminal"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Terminal &amp; Activity"
						android:textSize="14sp"
						android:textColor="#FFFFFF"
						android:focusable="false"
						android:layout_marginStart="12dp" />
				</LinearLayout>
			</com.google.android.material.card.MaterialCardView>
			<com.google.android.material.card.MaterialCardView
				android:id="@+id/nav_select_files"
				android:layout_width="match_parent"
				android:layout_height="52dp"
				android:layout_marginBottom="4dp"
				app:cardBackgroundColor="@android:color/transparent"
				app:cardCornerRadius="26dp"
				app:cardElevation="0dp"
				app:strokeWidth="0dp">
				<LinearLayout
					android:id="@+id/linear25"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:gravity="center_vertical"
					android:paddingHorizontal="16dp">
					<ImageView
						android:id="@+id/img_files"
						android:layout_width="20dp"
						android:layout_height="20dp"
						android:src="@drawable/icon_folder"
						android:scaleType="fitXY"
						android:focusable="false"
						app:tint="#9E9E9E" />
					<TextView
						android:id="@+id/txt_files"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Select Files"
						android:textSize="14sp"
						android:textColor="#FFFFFF"
						android:focusable="false"
						android:layout_marginStart="12dp" />
				</LinearLayout>
			</com.google.android.material.card.MaterialCardView>
			<com.google.android.material.card.MaterialCardView
				android:id="@+id/nav_switch_repo"
				android:layout_width="match_parent"
				android:layout_height="52dp"
				android:layout_marginBottom="4dp"
				app:cardBackgroundColor="@android:color/transparent"
				app:cardCornerRadius="26dp"
				app:cardElevation="0dp"
				app:strokeWidth="0dp">
				<LinearLayout
					android:id="@+id/linear26"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:gravity="center_vertical"
					android:paddingHorizontal="16dp">
					<ImageView
						android:id="@+id/img_switch"
						android:layout_width="20dp"
						android:layout_height="20dp"
						android:src="@drawable/ic_switch"
						android:scaleType="fitXY"
						android:focusable="false"
						app:tint="#9E9E9E" />
					<TextView
						android:id="@+id/txt_switch"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Switch Repo"
						android:textSize="14sp"
						android:textColor="#FFFFFF"
						android:focusable="false"
						android:layout_marginStart="12dp" />
				</LinearLayout>
			</com.google.android.material.card.MaterialCardView>
			<com.google.android.material.card.MaterialCardView
				android:id="@+id/nav_download_swb"
				android:layout_width="match_parent"
				android:layout_height="52dp"
				android:layout_marginBottom="4dp"
				app:cardBackgroundColor="@android:color/transparent"
				app:cardCornerRadius="26dp"
				app:cardElevation="0dp"
				app:strokeWidth="0dp">
				<LinearLayout
					android:id="@+id/linear27"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:gravity="center_vertical"
					android:paddingHorizontal="16dp">
					<ImageView
						android:id="@+id/img_download"
						android:layout_width="20dp"
						android:layout_height="20dp"
						android:src="@drawable/icon_download"
						android:scaleType="fitXY"
						android:focusable="false"
						app:tint="#9E9E9E" />
					<TextView
						android:id="@+id/txt_download"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Download .swb"
						android:textSize="14sp"
						android:textColor="#FFFFFF"
						android:focusable="false"
						android:layout_marginStart="12dp" />
				</LinearLayout>
			</com.google.android.material.card.MaterialCardView>
			<com.google.android.material.card.MaterialCardView
				android:id="@+id/nav_about"
				android:layout_width="match_parent"
				android:layout_height="52dp"
				android:layout_marginBottom="4dp"
				app:cardBackgroundColor="@android:color/transparent"
				app:cardCornerRadius="26dp"
				app:cardElevation="0dp"
				app:strokeWidth="0dp">
				<LinearLayout
					android:id="@+id/linear28"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:gravity="center_vertical"
					android:paddingHorizontal="16dp">
					<ImageView
						android:id="@+id/img_about"
						android:layout_width="20dp"
						android:layout_height="20dp"
						android:src="@drawable/icon_error"
						android:scaleType="fitXY"
						android:focusable="false"
						app:tint="#9E9E9E" />
					<TextView
						android:id="@+id/txt_about"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="About"
						android:textSize="14sp"
						android:textColor="#FFFFFF"
						android:focusable="false"
						android:layout_marginStart="12dp" />
				</LinearLayout>
			</com.google.android.material.card.MaterialCardView>
		</LinearLayout>
	</ScrollView>
	<TextView
		android:id="@+id/textview16"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="20dp"
		android:gravity="center_horizontal|center_vertical"
		android:text="CodeBySonu | v2.0.0"
		android:textSize="10sp"
		android:textColor="#444444"
		android:focusable="false" />
</LinearLayout>
